<!DOCTYPE dataset SYSTEM "dataset.dtd">
<dataset>	
	<table name="guiwindow">
		<column>id</column>
		<column>version</column>
		<column>positionX</column>
		<column>positionY</column>
		<column>width</column>
		<column>height</column>
		<column>rotation</column>
		<row>
			<value description="id">-1</value>
			<value description="version">1</value>
			<value description="positionX">100</value>
			<value description="positionY">200</value>
			<value description="width">400</value>
			<value description="height">300</value>
			<value description="rotation">0.0</value>
		</row>
		<row>
			<value description="id">-2</value>
			<value description="version">1</value>
			<value description="positionX">500</value>
			<value description="positionY">400</value>
			<value description="width">300</value>
			<value description="height">100</value>
			<value description="rotation">30.0</value>
		</row>
	</table>

	<table name="person">
		<column>id</column>
		<column>version</column>
		<column>firstName</column>
		<column>lastName</column>
		<row>
			<value description="id">-1</value>
			<value description="version">1</value>
			<value description="firstName">Mr.</value>
			<value description="lastName">X</value>
		</row>
		<row>
			<value description="id">-2</value>
			<value description="version">1</value>
			<value description="firstName">Mrs.</value>
			<value description="lastName">Y</value>
		</row>
	</table>
</dataset>